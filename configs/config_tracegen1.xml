<simulation starttime="0 min" endtime="10 min" warmup="90 sec">
	<world type="roadnet">
		<file name="maps/tl_2008_13089_edges.shp"/>
	</world>
	
	<agents count="1000">
		<mobilitymodel type="RandomTripRoadnet" filename="aa1k.m.trace">
			<parking type="gaussian" mean="15 sec" stdev="5 sec"/>
			<locationdistribution type="uniformroadnet"/>
			
			<speeddistribution type="gaussian" mean="55 mph" stdev="11 mph"/>
		</mobilitymodel>
		<periodictraceoutput period="60 sec" filename="a1k.m.pos.trace"/>
	</agents>
</simulation>